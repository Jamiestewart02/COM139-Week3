class Main {
  public static double area(double side) {
    double area = Math.pow(side,2);
   return area; 
  }
  public static double area(double length, double width) {
    double area = length * width;
   return area; 
  }

  public static void main(String[] args) {
    double square = area(13);
    double rectangle = area(12,12);
    System.out.println("The area of the square was: " + square);
    System.out.println("The area of the square was: " + rectangle);

  }
  
}
